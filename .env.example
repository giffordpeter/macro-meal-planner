# Application Environment
NODE_ENV=development
ENVIRONMENT=development

# Database Configuration
DB_USER=macro_user
DB_PASSWORD=macro_password
DB_NAME=macro_meal_planner
DATABASE_URL=postgresql://${DB_USER}:${DB_PASSWORD}@localhost:5432/${DB_NAME}
DATABASE_SSL_ENABLED=false

# API Configuration
API_PORT=8000
API_HOST=0.0.0.0
LOG_LEVEL=debug
CORS_ORIGINS=http://localhost:3000

# Frontend Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000
PORT=3000

# Authentication
NEXTAUTH_SECRET=development_secret
NEXTAUTH_URL=http://localhost:3000
AUTH_GITHUB_ID=your-github-client-id
AUTH_GITHUB_SECRET=your-github-client-secret
JWT_SECRET=development_secret
JWT_EXPIRATION=24h
REFRESH_TOKEN_EXPIRATION=7d

# AWS Configuration
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_DEFAULT_REGION=us-east-1

# LocalStack Configuration (Local AWS Services)
AWS_ENDPOINT=http://localhost:4566
LOCALSTACK_HOSTNAME=localhost
LOCALSTACK_EDGE_PORT=4566

# S3 Configuration
UPLOAD_BUCKET_NAME=macro-meal-planner-uploads-local

# SNS Configuration
NOTIFICATION_TOPIC_ARN=arn:aws:sns:us-east-1:000000000000:macro-meal-planner-notifications-local

# Secrets Manager Configuration
SECRETS_PREFIX=/macro-meal-planner/local

# Development Features
ENABLE_SWAGGER=true
ENABLE_DEBUG_TOOLBAR=true
ENABLE_DOCS=true

# Testing Configuration
TEST_DB_URL=postgresql://test_user:test_password@localhost:5432/macro_meal_planner_test

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key
