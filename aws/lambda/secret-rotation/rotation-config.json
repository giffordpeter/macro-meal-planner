{
    "RotationType": "POSTGRES_MULTI_USER",
    "RotationRules": {
        "AutomaticallyAfterDays": 30
    },
    "RotationLambdaARN": "${ROTATION_LAMBDA_ARN}",
    "RotateImmediately": false,
    "RotationConfig": {
        "Monitoring": {
            "AlertTopicArn": "${ALERT_TOPIC_ARN}",
            "NotificationEvents": [
                "RotationSuccess",
                "RotationFailed"
            ],
            "LogLevel": "INFO"
        }
    }
}
